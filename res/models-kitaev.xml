<model name="honeycomb-kitaev">
	<!-- Heisenberg-Kitaev model on the honeycomb lattice -->
	<interaction parameter="j" from="0,0,0,0" to="0,0,0,1" type="heisenberg" />
	<interaction parameter="j" from="0,0,0,0" to="0,-1,0,1" type="heisenberg" />
	<interaction parameter="j" from="0,0,0,0" to="-1,0,0,1" type="heisenberg" />
	<interaction parameter="k" from="0,0,0,0" to="0,0,0,1" type="xx" />
	<interaction parameter="k" from="0,0,0,0" to="0,-1,0,1" type="yy" />
	<interaction parameter="k" from="0,0,0,0" to="-1,0,0,1" type="zz" />
</model>

<model name="honeycomb-kitaev-gamma">
	<!-- Heisenberg-Kitaev-Gamma model on the honeycomb lattice -->
	<interaction parameter="j" from="0,0,0,0" to="0,0,0,1" type="heisenberg" />
	<interaction parameter="j" from="0,0,0,0" to="0,-1,0,1" type="heisenberg" />
	<interaction parameter="j" from="0,0,0,0" to="-1,0,0,1" type="heisenberg" />
	<interaction parameter="k" from="0,0,0,0" to="0,0,0,1" type="xx" />
	<interaction parameter="k" from="0,0,0,0" to="0,-1,0,1" type="yy" />
	<interaction parameter="k" from="0,0,0,0" to="-1,0,0,1" type="zz" />
	<interaction parameter="g" from="0,0,0,0" to="0,0,0,1" type="gx" />
	<interaction parameter="g" from="0,0,0,0" to="0,-1,0,1" type="gy" />
	<interaction parameter="g" from="0,0,0,0" to="-1,0,0,1" type="gz" />
</model>

<model name="hyperhoneycomb-kitaev">
	<!-- Heisenberg-Kitaev model on the hyperhoneycomb lattice -->
	<interaction parameter="j" from="0,0,0,0" to="0,0,0,1" type="heisenberg" />
	<interaction parameter="j" from="0,0,0,0" to="0,0,0,3" type="heisenberg" />
	<interaction parameter="j" from="0,0,0,0" to="1,0,0,3" type="heisenberg" />
	<interaction parameter="j" from="0,0,0,1" to="0,0,0,2" type="heisenberg" />
	<interaction parameter="j" from="0,0,0,1" to="0,-1,0,2" type="heisenberg" />
	<interaction parameter="j" from="0,0,0,2" to="0,0,1,3" type="heisenberg" />
	<interaction parameter="k" from="0,0,0,0" to="0,0,0,1" type="zz" />
	<interaction parameter="k" from="0,0,0,0" to="0,0,0,3" type="xx" />
	<interaction parameter="k" from="0,0,0,0" to="1,0,0,3" type="yy" />
	<interaction parameter="k" from="0,0,0,1" to="0,0,0,2" type="xx" />
	<interaction parameter="k" from="0,0,0,1" to="0,-1,0,2" type="yy" />
	<interaction parameter="k" from="0,0,0,2" to="0,0,1,3" type="zz" />
</model>

<model name="hyperhoneycomb-kitaev-gamma">
	<!-- Heisenberg-Kitaev-Gamma model on the hyperhoneycomb lattice -->
	<interaction parameter="j" from="0,0,0,0" to="0,0,0,1" type="heisenberg" />
	<interaction parameter="j" from="0,0,0,0" to="0,0,0,3" type="heisenberg" />
	<interaction parameter="j" from="0,0,0,0" to="1,0,0,3" type="heisenberg" />
	<interaction parameter="j" from="0,0,0,1" to="0,0,0,2" type="heisenberg" />
	<interaction parameter="j" from="0,0,0,1" to="0,-1,0,2" type="heisenberg" />
	<interaction parameter="j" from="0,0,0,2" to="0,0,1,3" type="heisenberg" />
	<interaction parameter="k" from="0,0,0,0" to="0,0,0,1" type="zz" />
	<interaction parameter="k" from="0,0,0,0" to="0,0,0,3" type="xx" />
	<interaction parameter="k" from="0,0,0,0" to="1,0,0,3" type="yy" />
	<interaction parameter="k" from="0,0,0,1" to="0,0,0,2" type="xx" />
	<interaction parameter="k" from="0,0,0,1" to="0,-1,0,2" type="yy" />
	<interaction parameter="k" from="0,0,0,2" to="0,0,1,3" type="zz" />
	<interaction parameter="g" from="0,0,0,0" to="0,0,0,1" type="gz" />
	<interaction parameter="g" from="0,0,0,0" to="0,0,0,3" type="gx" />
	<interaction parameter="g" from="0,0,0,0" to="1,0,0,3" type="gy" />
	<interaction parameter="g" from="0,0,0,1" to="0,0,0,2" type="gx" />
	<interaction parameter="g" from="0,0,0,1" to="0,-1,0,2" type="gy" />
	<interaction parameter="g" from="0,0,0,2" to="0,0,1,3" type="gz" />
</model>