<unitcell name="square">
	<primitive x="1" y="0" z="0" />
	<primitive x="0" y="1" z="0" />
	<primitive x="0" y="0" z="1" />

	<site x="0" y="0" z="0" />

	<bond from="0" to="0" dx="1" dy="0" dz="0" />
	<bond from="0" to="0" dx="0" dy="1" dz="0" />
</unitcell>

<unitcell name="triangular">
	<primitive x="sqrt(3)/2" y="-1/2" z="0" />
	<primitive x="sqrt(3)/2" y="1/2" z="0" />
	<primitive x="0" y="0" z="1" />

	<site x="0" y="0" z="0" />

	<bond from="0" to="0" dx="0" dy="-1" dz="0" />
	<bond from="0" to="0" dx="-1" dy="0" dz="0" />
	<bond from="0" to="0" dx="-1" dy="1" dz="0" />
</unitcell>

<unitcell name="honeycomb">
	<primitive x="3/2" y="sqrt(3)/2" z="0" />
	<primitive x="3/2" y="-sqrt(3)/2" z="0" />
	<primitive x="0" y="0" z="1" />

	<site x="0" y="0" z="0" />
	<site x="1" y="0" z="0" />

	<bond from="0" to="1" dx="0" dy="0" dz="0" />
	<bond from="1" to="0" dx="1" dy="0" dz="0" />
	<bond from="1" to="0" dx="0" dy="1" dz="0" />
</unitcell>

<unitcell name="kagome">
	<primitive x="2" y="0" z="0" />
	<primitive x="1" y="sqrt(3)" z="0" />
	<primitive x="0" y="0" z="1" />

	<site x="0" y="0" z="0" />
	<site x="1" y="0" z="0" />
	<site x="1/2" y="sqrt(3)/2" z="0" />

	<bond from="0" to="1" dx="0" dy="0" dz="0" />
	<bond from="0" to="2" dx="0" dy="0" dz="0" />
	<bond from="1" to="2" dx="0" dy="0" dz="0" />
	<bond from="1" to="0" dx="1" dy="0" dz="0" />
	<bond from="2" to="0" dx="0" dy="1" dz="0" />
	<bond from="2" to="1" dx="-1" dy="1" dz="0" />
</unitcell>

<unitcell name="cubic">
	<primitive x="1" y="0" z="0" />
	<primitive x="0" y="1" z="0" />
	<primitive x="0" y="0" z="1" />

	<site x="0" y="0" z="0" />

	<bond from="0" to="0" dx="1" dy="0" dz="0" />
	<bond from="0" to="0" dx="0" dy="1" dz="0" />
	<bond from="0" to="0" dx="0" dy="0" dz="1" />
</unitcell>

<unitcell name="fcc">
	<primitive x="0" y="1/sqrt(2)" z="1/sqrt(2)" />
	<primitive x="1/sqrt(2)" y="0" z="1/sqrt(2)" />
	<primitive x="1/sqrt(2)" y="1/sqrt(2)" z="0" />

	<site x="0" y="0" z="0" />

	<bond from="0" to="0" dx="1" dy="0" dz="0" />
	<bond from="0" to="0" dx="0" dy="1" dz="-1" />
	<bond from="0" to="0" dx="0" dy="1" dz="0" />
	<bond from="0" to="0" dx="-1" dy="0" dz="1" />
	<bond from="0" to="0" dx="0" dy="0" dz="1" />
	<bond from="0" to="0" dx="-1" dy="1" dz="0" />
</unitcell>

<unitcell name="diamond">
	<primitive x="0" y="2/sqrt(3)" z="2/sqrt(3)" />
