	<primitive x="sqrt(8)/2" y="sqrt(8)/2" z="sqrt(8)/2" />

	<site x="1/sqrt(8)" y="1/sqrt(8)" z="1/sqrt(8)" />
	<site x="5/sqrt(8)" y="3/sqrt(8)" z="-1/sqrt(8)" />
	<site x="3/sqrt(8)" y="1/sqrt(8)" z="-1/sqrt(8)" />
	<site x="7/sqrt(8)" y="3/sqrt(8)" z="1/sqrt(8)" />

	<bond from="0" to="2" dx="0" dy="0" dz="0" />
	<bond from="1" to="2" dx="0" dy="0" dz="0" />
	<bond from="1" to="3" dx="0" dy="0" dz="0" />
	<bond from="1" to="0" dx="0" dy="1" dz="0" />
	<bond from="3" to="2" dx="0" dy="0" dz="1" />
	<bond from="3" to="0" dx="1" dy="0" dz="0" />
</unitcell>

