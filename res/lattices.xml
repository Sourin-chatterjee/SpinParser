	<primitive x="2/sqrt(3)" y="2/sqrt(3)" z="0" />

	<site x="0" y="0" z="0" />
	<site x="1/sqrt(3)" y="1/sqrt(3)" z="1/sqrt(3)" />

	<bond from="0" to="1" dx="0" dy="0" dz="0" />
	<bond from="1" to="0" dx="1" dy="0" dz="0" />
	<bond from="1" to="0" dx="0" dy="1" dz="0" />
	<bond from="1" to="0" dx="0" dy="0" dz="1" />
</unitcell>

<unitcell name="hyperoctagon">
